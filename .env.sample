# 데이터베이스 설정
# 로컬 개발 시 Prisma PostgreSQL 서버 연결 문자열
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/sona_db?schema=public"

# JWT 설정
JWT_SECRET="your-jwt-secret-key-change-in-production"
JWT_EXPIRES_IN="24h"

# 애플리케이션 설정
PORT=3000
NODE_ENV="development"

# 프론트엔드 URL (CORS 설정용)
FRONTEND_URL="http://localhost:5173"
